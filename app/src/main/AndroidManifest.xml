<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gace.app">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/thelogo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/thelogo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity android:name=".BuyTicket"
            android:theme="@style/AppTheme.YesActionBar">

        </activity>
        <activity android:name=".Reset_password" />
        <activity
            android:name=".MyProfile"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.YesActionBar" />
        <activity
            android:name=".UploadEvent"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.YesActionBar" />
        <activity
            android:name=".EventRegistration"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.YesActionBar" />
        <activity
            android:name=".Eventinfo"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.YesActionBar"/>
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.YesActionBar" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity" />
    </application>

</manifest>